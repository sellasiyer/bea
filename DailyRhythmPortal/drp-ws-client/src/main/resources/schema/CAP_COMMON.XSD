	<xs:complexType name="UpgradeQualificationType">
		<xs:sequence>
			<xs:element name="eligible" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Whether this ptn is allowed for upgrade or not.
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="qualificationDescription" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						This is a short description of the upgrade
						qualification details.

						To be based on tier level.

						If Tier 1 THEN Early Upgrade.

						If Tier 2 Then Standard Upgrade.

						If Tier 0 Then Not Eligible for Upgrade.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="EARLY_UPGRADE"/>
						<xs:enumeration value="STANDARD_UPGRADE"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="equipmentRestrictions" type="EquipmentRestrictionsType" maxOccurs="1"
				minOccurs="0"> </xs:element>
			<xs:element name="eligibilityDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The date that the device became or will become
						eligible.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="altUpgradeEligible" maxOccurs="1" minOccurs="0" type="xs:boolean">
		        <xs:annotation>
		          <xs:documentation>
		            Required for Vez only. Flag to identify customer's Alternate upgrade Eligible
		          </xs:documentation>
		        </xs:annotation>
    	   </xs:element>


			<xs:element name="qualificationLevel" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The customer's qualification level.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="2"/>
						<xs:maxLength value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="qualificationType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The customer's qualification type.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="B"/>
						<xs:enumeration value="S"/>
						<xs:enumeration value="O"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="minimumMRC" type="CurrencyAmountInfo">
				<xs:annotation>
					<xs:documentation>
						The minimum MRC that will be required in order
						to do the upgrade. If provided, the plans list
						will be filtered by this value.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="maximumMRC" type="CurrencyAmountInfo">
				<xs:annotation>
					<xs:documentation>
						The maximum MRC that can be selected when doing
						the upgrade. If provided, the plans list will be
						filtered by this value.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="waiveUpgradeFee" type="xs:boolean" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates if the upgrade fee will be waived. Do
						not send in request in Plans Request.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="minimumCommitment" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates the minimum contract term that must be
						selected in order to do the upgrade. If provided
						the plans list will be filtered by this value.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:totalDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="qualificationMessage" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Additional information about the upgrade. Do not
						send in request in Plans Request.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="policyCategory" type="PolicyCategoryInfo" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Additional information about the upgrade. Do not
						send in request in Plans Request.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricePlanList" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Code that represents a group of plans that are
						valid for the upgrade. If provided the plans
						list will be filtered by this value.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="upgradeFee" type="CurrencyAmountInfo" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						This element is used for TMO.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ipAddressType">
		<xs:annotation>
			<xs:documentation>
				The IP address. This is used for debugging purposes.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern
				value="((0|1[0-9]{0,2}|2([0-4][0-9]?|5[0-5]?|[6-9])?|[3-9][0-9]?)\.){3}(0|1[0-9]{0,2}|2([0-4][0-9]?|5[0-5]?|[6-9])?|[3-9][0-9]?)"
				> </xs:pattern>
		</xs:restriction>
	</xs:simpleType>

