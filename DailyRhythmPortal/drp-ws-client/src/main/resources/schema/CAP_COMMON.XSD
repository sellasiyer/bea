<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://bestbuy.com/bbym/beast/cap/common"
	targetNamespace="http://bestbuy.com/bbym/beast/cap/common" elementFormDefault="unqualified"
	attributeFormDefault="unqualified">
	<xs:simpleType name="AccountNumberType">
		<xs:annotation>
			<xs:documentation>
				Account number for an existing account. Connditionally
				required, see the doc for more information.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountSubNumberType">
		<xs:annotation>
			<xs:documentation>
				Account sub number for an existing account.
				Connditionally required, see the doc for more
				information.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SubscriberIdType">
		<xs:annotation>
			<xs:documentation>
				Subscriber id of the customer at the carrier's end.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="orderDateType">
		<xs:annotation>
			<xs:documentation>
				The date/time that the order was created by the
				customer. Format is "CCYY-MM-DDThh:mm:ss" (T
				is the date/time separator)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="PhoneNumberType">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{10}"/>
		</xs:restriction>
		<!--Format for phone number is xxxxxxxxxx.-->
	</xs:simpleType>
	<xs:simpleType name="MaxStringOf10">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SsnType">
		<xs:annotation>
			<xs:documentation>Social security number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="\d{4}|\d{9}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FederalTaxIdType">
		<xs:annotation>
			<xs:documentation>Federal tax id</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="\d{9}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="EsnType">
		<xs:annotation>
			<xs:documentation>
				Customer's requested CDMA phone identifier,
				required if model-id or hsn is not provided
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-zA-Z0-9]{1,20}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ModelIdType">
		<xs:annotation>
			<xs:documentation>
				Customer's requested phone model id, required if
				MEID or hsn is not provided. Or Device number/Instrument
				Model number Id. Used by Sprint
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zipCodeType">
		<xs:annotation>
			<xs:documentation>zip code (first 5).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="\d{5}"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CustomerType">
		<xs:annotation>
			<xs:documentation>
				The type of the customer, valid values are INDIVIDUAL,
				BUSINESS, CORPORATE_BUSINESS, CORPORATE_INDIVIDUAL
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="INDIVIDUAL"/>
			<xs:enumeration value="BUSINESS"/>
			<xs:enumeration value="CORPORATE_INDIVIDUAL"/>
			<xs:enumeration value="CORPORATE_BUSINESS"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OrderType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="NEW"/>
			<xs:enumeration value="UPGRADE"/>
			<xs:enumeration value="ADD_ON"/>
			<xs:enumeration value="ESN_SWAP"/>
			<xs:enumeration value="PLAN_CHANGE"/>
			<xs:enumeration value="CONVERT"/>
			<xs:enumeration value="QUERY"/>
			<xs:enumeration value="TOPUP"/>
			<xs:enumeration value="RETURN"/>
			<xs:enumeration value="EXCHANGE"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="Order">
		<xs:annotation>
			<xs:documentation>
				Populated during Activation only for upgrade/replacement
				orders. It should include the reference-ptn of the
				subscriber changing their handset
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="accountNumber" minOccurs="0" type="AccountNumberType">
				<xs:annotation>
					<xs:documentation>
						Account number for an existing account.
						Connditionally required, see the doc for more
						information.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountSubNumber" minOccurs="0" type="AccountSubNumberType">
				<xs:annotation>
					<xs:documentation>
						Account sub-number for an existing account.
						Required in the in the Service Validation
						Request Used for Verizon
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountPhoneNumber" minOccurs="0" type="PhoneNumberType">
				<xs:annotation>
					<xs:documentation>
						In a single line order, the phone number for the
						line that is being modified.

						In a multi-line order, the primary line should
						be indicated, otherwise any existing number that
						is being acted on.

						Connditionally required, see the doc for more
						information.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="authorizedUsers"
				type="AuthorizedUsersType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Complex Type to capture the name of the
						authorized users on the account.

						Used for fraud protection
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Name">
		<xs:annotation>
			<xs:documentation>
				Name of the person associated to the shipping address
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>

			<xs:element name="firstName">
				<xs:annotation>
					<xs:documentation>
						The customer's first name
					</xs:documentation>

				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="middleInitial" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The customer's middle initial.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lastName" type="FirstLastNameType">
				<xs:annotation>
					<xs:documentation>
						The customer's last name
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="suffix" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The customer's suffix. Example: JR, SR, III
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="prefix" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The customer's prefix. Example: Mr, Mrs, Ms
						For sprint - max length is 60.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="60"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="isMultiAccountName" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Boolean to notify if authorized user name on multiple accounts
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="numberOfAccounts" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Number of accounts user is listed on
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressType">
		<xs:sequence>
			<xs:element name="streetAddress1" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The customer's street address line 1.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="streetAddress2" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The customer's street address line 2.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="city">
				<xs:annotation>
					<xs:documentation>
						The customer's city.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="stateCode">
				<xs:annotation>
					<xs:documentation>
						The customer's state.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="zipCode" type="zipCodeType">
				<xs:annotation>
					<xs:documentation>
						zip code (first 5).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zipCode4" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The customer's zip code (last 4).
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:whiteSpace value="collapse"/>
						<xs:pattern value="\d{4}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="countryCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The customer's country.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="US"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="urbanizationCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						An urbanization denotes an area, sector, or
						residential development within a geographic
						area. In addition to being a descriptive word,
						it precedes the name of that area. This
						descriptor, commonly used in Puerto Rican urban
						areas, is an important part of the addressing
						format as it describes the location of a given
						street.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="direction" maxOccurs="1" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="North"/>
						<xs:enumeration value="South"/>
						<xs:enumeration value="East"/>
						<xs:enumeration value="West"/>
						<xs:enumeration value="NE"/>
						<xs:enumeration value="SE"/>
						<xs:enumeration value="NW"/>
						<xs:enumeration value="SW"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="StreetType">
		<xs:sequence>
			<xs:element name="unit" type="UnitInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Complex type Part of the address that is used to
						uniquely identify a unit in a multi-resident
						dwelling.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetNum" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The customer's street number.

						Example:4500
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetName" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The customer's street name. Should include
						the street type. Should include the street
						designator. Ex: Penn Ave
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dir" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The customer's street direction.

						Example:South
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
						<xs:enumeration value="E"/>
						<xs:enumeration value="N"/>
						<xs:enumeration value="NE"/>
						<xs:enumeration value="NW"/>
						<xs:enumeration value="S"/>
						<xs:enumeration value="SE"/>
						<xs:enumeration value="SW"/>
						<xs:enumeration value="W"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="POType">
		<xs:sequence>
			<xs:element name="pobox">
				<xs:annotation>
					<xs:documentation>
						Post Office Box Number.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RuralType">
		<xs:sequence>
			<xs:element name="ruralRte" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The customer's Rural Route number.
						Connditionally required, see the doc for more
						information.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ruralDel" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The customer's rural delivery box number.
						Connditionally required, see the doc for more
						information.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="UnitInfo">
		<xs:sequence>
			<xs:element name="unitNumber">
				<xs:annotation>
					<xs:documentation>
						The unit number. Example: 100
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="unitDesignator">
				<xs:annotation>
					<xs:documentation>
						Type of Unit

						Example: Unit

					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
						<xs:enumeration value="APT"/>
						<xs:enumeration value="BLDG"/>
						<xs:enumeration value="DEPT"/>
						<xs:enumeration value="FL"/>
						<xs:enumeration value="HNGR"/>
						<xs:enumeration value="LOT"/>
						<xs:enumeration value="PIER"/>
						<xs:enumeration value="RM"/>
						<xs:enumeration value="SLIP"/>
						<xs:enumeration value="STE"/>
						<xs:enumeration value="SPC"/>
						<xs:enumeration value="STOP"/>
						<xs:enumeration value="TRLR"/>
						<xs:enumeration value="UNIT"/>
						<xs:enumeration value="BSMT"/>
						<xs:enumeration value="FRNT"/>
						<xs:enumeration value="OFC"/>
						<xs:enumeration value="PH"/>
						<xs:enumeration value="REAR"/>
						<xs:enumeration value="LOWR"/>
						<xs:enumeration value="LBBY"/>
						<xs:enumeration value="SIDE"/>
						<xs:enumeration value="UPPR"/>
						<xs:enumeration value="#"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="IntlBusinessHierarchyType">
		<xs:sequence>
			<xs:element name="enterprise">
				<xs:annotation>
					<xs:documentation>
						The overall Best Buy. INC Corporation
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
						<xs:enumeration value="Best Buy Enterprise"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="tradingArea">
				<xs:annotation>
					<xs:documentation>
						A distinct Geographic region comprising of one
						or more countries: US, Canada, China, UK,
						Mexico. This will be implemented as the ISO
						Standard 3166: 2 Character Country code.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="2"/>
						<xs:enumeration value="US"/>
						<xs:enumeration value="CA"/>
						<xs:enumeration value="CN"/>
						<xs:enumeration value="GB"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="company">
				<xs:annotation>
					<xs:documentation>
						One or more legal entities which reside /
						operate in the particular countries under a
						given Trading Area.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
						<xs:enumeration value="Best Buy LLP"/>
						<xs:enumeration value="Pacific Sales"/>
						<xs:enumeration value="Five-Star Appliance"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="brand">
				<xs:annotation>
					<xs:documentation>
						A distinct banner under which business is
						operated (usually visible to the customer)

						This is not an Enumeration anymore and should be
						sent correctly by the client requesting the
						service

						All clients except Whitelabel and Upgrade
						Checker should send it as Best Buy. WL &amp; UC
						will have multiple Brands over time.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="businessUnit">
				<xs:annotation>
					<xs:documentation>
						An organizational sub unit which may hold
						P&amp;L responsibility
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
						<xs:enumeration value="Best Buy Mobile"/>
						<xs:enumeration value="Whitelabel"/>
						<xs:enumeration value="Online"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="channel">
				<xs:annotation>
					<xs:documentation>
						The medium under which business is operated
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Store"/>
						<xs:enumeration value="Web"/>
						<xs:enumeration value="Call-Center"/>
						<xs:enumeration value="Kiosk"/>
						<xs:enumeration value="Best Buy Live"/>
						<xs:enumeration value="Remix"/>
						<xs:enumeration value="STORE"/>
						<xs:enumeration value="WEB"/>
						<xs:enumeration value="CCR"/>
						<xs:enumeration value="KIOSK"/>
						<xs:enumeration value="RemixAPI"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CreditCardInfo">
		<xs:sequence>
			<xs:element name="type" type="PaymentCardType"/>
			<xs:element name="number">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation>
							Credit card number
						</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:minLength value="14"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="securityCode">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation>
							Credit card security code
						</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:minLength value="3"/>
						<xs:maxLength value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="expiration" type="xs:gYearMonth">
				<xs:annotation>
					<xs:documentation>
						Format for expiration date is
						"CCYY-MM"
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cardAddressZip" type="zipCodeType" maxOccurs="1" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="PaymentCardType">
		<xs:annotation>
			<xs:documentation>
				Credit card type values are V (Visa), M (Mastercard), A
				(American Express), D (Discover), DC (Diner's Club)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="V"/>
			<xs:enumeration value="M"/>
			<xs:enumeration value="A"/>
			<xs:enumeration value="D"/>
			<xs:enumeration value="DC"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PlanType">
		<xs:annotation>
			<xs:documentation>
