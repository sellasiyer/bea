        <xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.tsh.bestbuy.com/common/communication/v1" version="1.0" xmlns:com="http://www.tsh.bestbuy.com/common/v1" xmlns:dt="http://www.tsh.bestbuy.com/common/datatype/v1" xmlns:loc="http://www.tsh.bestbuy.com/common/location/v1" xmlns:qdt="http://www.tsh.bestbuy.com/common/qualifiled/datatype/v1" xmlns:tns="http://www.tsh.bestbuy.com/common/communication/v1" xmlns:xs="http://www.w3.org/2001/XMLSchema">
