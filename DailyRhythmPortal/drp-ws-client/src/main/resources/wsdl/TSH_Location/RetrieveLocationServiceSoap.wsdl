            <xs:annotation>
                <xs:documentation>Common Location schema is defined to capture common communication elements which can be leveraged by other TSH schemas.</xs:documentation>
            </xs:annotation>
            <xs:complexType name="CommunicationBaseType">
                <xs:annotation>
                    <xs:documentation>Communication Base Type to define basic communication methods</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element ref="com:ChannelCode">
                        <xs:annotation>
                            <xs:documentation>Channel code denotes the communication option used in the message </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:choice>
                        <xs:element minOccurs="0" name="Address" type="loc:MailingAddressType"/>
                        <xs:element minOccurs="0" ref="tns:EMail"/>
                        <xs:element minOccurs="0" ref="tns:Phone"/>
                    </xs:choice>
                </xs:sequence>
                <xs:attribute ref="com:operationCode"/>
            </xs:complexType>
