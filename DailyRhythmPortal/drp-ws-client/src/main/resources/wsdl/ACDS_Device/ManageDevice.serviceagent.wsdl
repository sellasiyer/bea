        <xs:schema xmlns:dt="http://www.tsh.bestbuy.com/common/datatype/v1" xmlns:tns="http://www.tsh.bestbuy.com/common/metadata/fields/v1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:qdt="http://www.tsh.bestbuy.com/common/qualifiled/datatype/v1" targetNamespace="http://www.tsh.bestbuy.com/common/metadata/fields/v1" version="1.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
