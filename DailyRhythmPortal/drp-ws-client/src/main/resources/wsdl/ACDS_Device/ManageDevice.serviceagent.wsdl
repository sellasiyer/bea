                <xs:annotation>
                    <xs:documentation xml:lang="en">
				A character string (letters, figures, or symbols) that for brevity and/or 
				languange independence may be used to represent or replace a definitive value
				or text of an attribute together with relevant supplementary information.
			</xs:documentation>
                </xs:annotation>
                <xs:simpleContent>
                    <xs:extension base="xs:normalizedString">
                        <xs:attribute name="name" type="xs:string">
                            <xs:annotation>
                                <xs:documentation xml:lang="en">
							The textual equivalent of the code content component.
						</xs:documentation>
                            </xs:annotation>
                        </xs:attribute>
                    </xs:extension>
                </xs:simpleContent>
            </xs:complexType>
            <xs:complexType name="FlagType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
				Flag Type
			</xs:documentation>
                </xs:annotation>
                <xs:simpleContent>
                    <xs:extension base="xs:string"/>
                </xs:simpleContent>
            </xs:complexType>
            <xs:complexType name="IdentifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
				A character string to identify and distinguish uniquely, one instance 
				of an object in an identification scheme from all other objects in the same 
				scheme together with relevant supplementary information.
			</xs:documentation>
                </xs:annotation>
                <xs:simpleContent>
                    <xs:extension base="xs:normalizedString">
                        <xs:attribute name="type" type="qdt:NormalizedStringType">
                            <xs:annotation>
                                <xs:documentation xml:lang="en">
							type assigned to the identifier.
						</xs:documentation>
                            </xs:annotation>
                        </xs:attribute>
                    </xs:extension>
                </xs:simpleContent>
            </xs:complexType>
            <xs:simpleType name="IndicatorType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
				A list of two mutually exclusive Boolean values that express the only possible state
				of a property.
			</xs:documentation>
                </xs:annotation>
                <xs:restriction base="qdt:BooleanType">
                    <xs:pattern value="false"/>
                    <xs:pattern value="true"/>
                    <xs:pattern value="0"/>
                    <xs:pattern value="1"/>
                    <xs:pattern value="yes"/>
                    <xs:pattern value="no"/>
                </xs:restriction>
            </xs:simpleType>
            <xs:complexType name="KeyType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
				A character string used to identify.
			</xs:documentation>
                </xs:annotation>
                <xs:simpleContent>
                    <xs:extension base="xs:normalizedString"/>
                </xs:simpleContent>
            </xs:complexType>
            <xs:complexType name="MeasureType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
				A numeric value determined by measuring an object along with the 
				specified unit of measure.
			</xs:documentation>
                </xs:annotation>
                <xs:simpleContent>
                    <xs:extension base="xs:decimal">
                        <xs:attribute name="unitCode" use="required" type="qdt:StringType">
                            <xs:annotation>
                                <xs:documentation xml:lang="en">
							The type of unit of measure.
						</xs:documentation>
                            </xs:annotation>
                        </xs:attribute>
                    </xs:extension>
                </xs:simpleContent>
            </xs:complexType>
            <xs:simpleType name="MinorVersionType">
                <xs:restriction base="xs:nonNegativeInteger">
                    <xs:minInclusive value="1"/>
                    <xs:maxInclusive value="99"/>
                </xs:restriction>
            </xs:simpleType>
            <xs:complexType name="NameType">
                <xs:simpleContent>
                    <xs:extension base="xs:string"/>
                </xs:simpleContent>
            </xs:complexType>
            <xs:complexType name="NameValueType">
                <xs:sequence>
                    <xs:element name="name" type="tns:KeyType"/>
                    <xs:element name="value" type="tns:TextType"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="SequencedNameType">
