        <xs:schema xmlns:dt="http://www.tsh.bestbuy.com/common/datatype/v1" xmlns:tns="http://tsh.bestbuy.com/tsh/tshfault" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cf="http://www.tsh.bestbuy.com/common/v1" targetNamespace="http://tsh.bestbuy.com/tsh/tshfault" elementFormDefault="qualified" attributeFormDefault="unqualified">
            <xs:import namespace="http://www.tsh.bestbuy.com/common/datatype/v1"/>
            <xs:import namespace="http://www.tsh.bestbuy.com/common/v1"/>
            <xs:annotation>
                <xs:documentation>-----------------------------------------------------------------------------------------------------
		</xs:documentation>
                <xs:documentation> Version: 1.0         </xs:documentation>
                <xs:documentation> Project Name : TagSuperHighWay </xs:documentation>
                <xs:documentation> Description : This XSD contains the exception
			format for TagSuperHighway Project. This file refers
			QualifiedDataType.xsd for the Data Types .  												</xs:documentation>
                <xs:documentation>-----------------------------------------------------------------------------------------------------
		</xs:documentation>
            </xs:annotation>
            <xs:annotation>
                <xs:documentation>This is for the service detail fields

			1. Service -
			Name of the service where the exception occurred.
			2. ProcessId -
			Instance Id of the process
			3. DomainName - This is the domain to
			Service belongs
			4. ServerName - The name of the Integration Server or
			Integration
			Hostname of where the service failed.
			5. ApplicationName -
			The name of the source/target system where the
			exception occurred.
			6.
			ExceptionCode - The internal exception code generated by the target
			application.
			7. ExceptionType - The type of exception identified by
			the fully
			qualified exception name
			8. ExceptionMessage - The complete
			exception message returned by the
			application or service.
			9.
			ExceptionAdditionalInfo - A field provided to store additional
			exception related information. An example would be a value indicating
			the level at which the exception occurred.
			10. LogMessage - A field
			provided to store integration process specific
			log data.
			11.
			ExceptionTimeStamp - The timestamp when the exception occurred

		</xs:documentation>
            </xs:annotation>
            <xs:simpleType name="FaultTypeEnumeration">
                <xs:annotation>
                    <xs:documentation>Enumerations for the Indicator for Error or Warning
			</xs:documentation>
                </xs:annotation>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="ERROR"/>
                    <xs:enumeration value="WARNING"/>
                </xs:restriction>
            </xs:simpleType>
            <xs:complexType name="TSHFaultType">
                <xs:annotation>
                    <xs:documentation>Provides information about TSH Fault generated for
				TSH service</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="SourceID" type="dt:TextType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation> Provides the SourceID of the TSH fault.
					</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SourceName" type="dt:TextType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Provide the Source Name from which the fault was
						generated.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Fault" type="tns:faultType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Returns information about the Fault Data like
						ErrorCode, Error Message.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Status" type="dt:TextType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Provides the status of TSH Fault.
					</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ServiceDetails" type="tns:serviceDetailsType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element ref="cf:UserArea">
                        <xs:annotation>
                            <xs:documentation>Allows the Tag Super Highway schema data model to extend the specification in order to provide additional information that is not captured in Canonical Schema.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="faultType">
                <xs:annotation>
                    <xs:documentation>Returns information about the Fault Data like
				ErrorCode, Error Message</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="Code" type="dt:TextType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Returns the Error code or the Exception Code.
					</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Message" type="dt:TextType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
						Returns the Error message based on the Error Code
						or Exception Code.
					</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Detail" type="dt:TextType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
						Returns the Detailed description of error which
						includes Error code and Error message.
					</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="type" type="tns:FaultTypeEnumeration"/>
            </xs:complexType>
            <xs:complexType name="serviceDetailsType">
                <xs:annotation>
                    <xs:documentation>Information about the Service details .
			</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="Service" type="dt:TextType" minOccurs="0"/>
                    <xs:element name="ProcessId" type="dt:TextType" minOccurs="0"/>
                    <xs:element name="DomainName" type="dt:TextType" minOccurs="0"/>
                    <xs:element name="ServerName" type="dt:TextType" minOccurs="0"/>
                    <xs:element name="ApplicationName" type="dt:TextType" minOccurs="0"/>
                    <xs:element name="ExceptionCode" type="dt:TextType" minOccurs="0"/>
                    <xs:element name="ExceptionType" type="dt:TextType" minOccurs="0"/>
                    <xs:element name="ExceptionMessage" type="dt:TextType" minOccurs="0"/>
                    <xs:element name="ExceptionAdditionalInfo" type="dt:TextType" minOccurs="0"/>
                    <xs:element name="LogMessage" type="dt:TextType" minOccurs="0"/>
                    <xs:element name="ExceptionTimeStamp" type="dt:TextType" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>
            <xs:element name="TSHFault" type="tns:TSHFaultType">
                <xs:annotation>
                    <xs:documentation>This XSD provides information about TSH Fault
				generated for TSH service</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:schema>
        <xs:schema xmlns:tns="http://www.tsh.bestbuy.com/common/datatype/v1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:qdt="http://www.tsh.bestbuy.com/common/qualifiled/datatype/v1" targetNamespace="http://www.tsh.bestbuy.com/common/datatype/v1" elementFormDefault="qualified" attributeFormDefault="unqualified">
            <xs:import namespace="http://www.tsh.bestbuy.com/common/qualifiled/datatype/v1"/>
