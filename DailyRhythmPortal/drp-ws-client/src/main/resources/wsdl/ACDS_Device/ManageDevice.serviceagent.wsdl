                    <xs:extension base="tns:NameType">
                        <xs:attribute name="sequence" type="xs:positiveInteger"/>
                        <xs:attribute name="sequenceName" type="xs:normalizedString"/>
                    </xs:extension>
                </xs:simpleContent>
            </xs:complexType>
            <xs:complexType name="SequencedTextType">
                <xs:simpleContent>
                    <xs:extension base="tns:TextType">
                        <xs:attribute name="sequence" type="xs:positiveInteger"/>
                        <xs:attribute name="sequenceName" type="xs:normalizedString"/>
                    </xs:extension>
                </xs:simpleContent>
            </xs:complexType>
            <xs:complexType name="TextType">
                <xs:simpleContent>
                    <xs:extension base="xs:string"/>
                </xs:simpleContent>
            </xs:complexType>
            <xs:complexType name="UserAreaType">
                <xs:sequence>
                    <xs:any minOccurs="0" maxOccurs="unbounded" processContents="strict" namespace="##any"/>
                </xs:sequence>
            </xs:complexType>
            <xs:element name="ENTER_NAME_OF_ROOT_ELEMENT_HERE" type="xs:anyType">
