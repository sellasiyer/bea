    <xs:complexType name="MatchPartyRequestType">
        <xs:complexContent>
            <xs:extension base="BaseRequestType">
                <xs:sequence>
                    <xs:element name="Party" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="PartyIdentity" type="IdentityType" minOccurs="0"/>
                                <xs:element name="PartyDetail" minOccurs="0">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element name="Alias" type="AliasType" minOccurs="0"/>
                                            <xs:element name="Name" type="NameType" minOccurs="0"/>
                                            <xs:element name="CommunicationAddress" minOccurs="0" maxOccurs="unbounded">
                                                <xs:complexType>
                                                    <xs:choice>
                                                        <xs:element name="PhoneAddress" type="PhoneType" minOccurs="0"/>
                                                        <xs:element name="PostalAddress" type="AddressType" minOccurs="0"/>
                                                        <xs:element name="EmailAddress" type="EmailType" minOccurs="0"/>
                                                    </xs:choice>
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                            <xs:attribute name="Id" type="IdType"/>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Configuration" minOccurs="0">
